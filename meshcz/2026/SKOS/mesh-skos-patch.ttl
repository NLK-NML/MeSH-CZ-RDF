# Custom MeSH SKOS patches
##  - version 1.1.0 2026-02-07
PREFIX mesh:   <http://id.nlm.nih.gov/mesh/>
PREFIX meshv:  <http://id.nlm.nih.gov/mesh/vocab#>
PREFIX mesht:  <http://www.medvik.cz/schema/mesh/vocab/#>
PREFIX meshx:  <http://mesh.medvik.cz/>
PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos:   <http://www.w3.org/2004/02/skos/core#>
PREFIX skosext: <http://purl.org/finnonto/schema/skosext#>
PREFIX owl:    <http://www.w3.org/2002/07/owl#>

mesh: a skos:ConceptScheme ;
    skos:prefLabel "Medical Subject Headings 2026"@en ;
    skos:prefLabel "Medical Subject Headings 2026"@cs ;
    skos:prefLabel "Medical Subject Headings 2026"@fr .

mesht:allowedQualifier
    a rdf:Property ;
    rdfs:subPropertyOf skos:related ;
    rdfs:domain skos:Concept ;
    rdfs:range  skos:Concept ;
    rdfs:label "Allowed qualifier"@en ;
    rdfs:label "Povolené podheslo"@cs ;
    rdfs:label "Qualificatif autorisé"@fr .
    
mesht:extUrl
    a rdf:Property ;
    rdfs:label "Related record"@en ;
    rdfs:label "Související záznam"@cs ;
    rdfs:label "Affichage associé"@fr .
  
meshx:scheme
    a skos:ConceptScheme ;
    skos:prefLabel "MeSH Categories"@en ;
    skos:prefLabel "MeSH Kategorie"@cs ;
    skos:prefLabel "MeSH Catégories"@fr ;
    skos:hasTopConcept
        meshx:A ,
        meshx:B ,
        meshx:C ,
        meshx:D ,
        meshx:E ,
        meshx:F ,
        meshx:G ,
        meshx:H ,
        meshx:I ,
        meshx:J ,
        meshx:K ,
        meshx:L ,
        meshx:M ,
        meshx:N ,
        meshx:V ,
        meshx:Y ,
        meshx:Z .

meshx:A
    a skos:Concept ;
    skos:notation "A" ;
    skos:prefLabel "Anatomy"@en ;
    skos:prefLabel "Anatomie"@cs ;
    skos:prefLabel "Anatomie"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:B
    a skos:Concept ;
    skos:notation "B" ;
    skos:prefLabel "Organisms"@en ;
    skos:prefLabel "Organismy"@cs ;
    skos:prefLabel "Organismes"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:C
    a skos:Concept ;
    skos:notation "C" ;
    skos:prefLabel "Diseases"@en ;
    skos:prefLabel "Nemoci"@cs ;
    skos:prefLabel "Maladies"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:D
    a skos:Concept ;
    skos:notation "D" ;
    skos:prefLabel "Chemicals and Drugs"@en ;
    skos:prefLabel "Chemikálie a léčiva"@cs ;
    skos:prefLabel "Produits chimiques et pharmaceutiques"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:E
    a skos:Concept ;
    skos:notation "E" ;
    skos:prefLabel "Analytical, Diagnostic and Therapeutic Techniques and Equipment"@en ;
    skos:prefLabel "Analytické, diagnostické a terapeutické techniky a přístroje"@cs ;
    skos:prefLabel "Techniques analytiques, diagnostiques, thérapeutiques et équipements"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:F
    a skos:Concept ;
    skos:notation "F" ;
    skos:prefLabel "Psychiatry and Psychology"@en ;
    skos:prefLabel "Psychiatrie a psychologie"@cs ;
    skos:prefLabel "Psychiatrie et psychologie"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:G
    a skos:Concept ;
    skos:notation "G" ;
    skos:prefLabel "Phenomena and Processes"@en ;
    skos:prefLabel "Jevy a procesy"@cs ;
    skos:prefLabel "Phénomènes et processus"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:H
    a skos:Concept ;
    skos:notation "H" ;
    skos:prefLabel "Disciplines and Occupations"@en ;
    skos:prefLabel "Obory a povolání"@cs ;
    skos:prefLabel "Disciplines et professions"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:I
    a skos:Concept ;
    skos:notation "I" ;
    skos:prefLabel "Anthropology, Education, Sociology and Social Phenomena"@en ;
    skos:prefLabel "Antropologie, vzdělávání, sociologie a sociální jevy"@cs ;
    skos:prefLabel "Anthropologie, enseignement, sociologie et phénomènes sociaux"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:J
    a skos:Concept ;
    skos:notation "J" ;
    skos:prefLabel "Technology, Industry, Agriculture"@en ;
    skos:prefLabel "Technologie, průmysl a zemědělství"@cs ;
    skos:prefLabel "Technologie, industrie et agriculture"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:K
    a skos:Concept ;
    skos:notation "K" ;
    skos:prefLabel "Humanities"@en ;
    skos:prefLabel "Humanitní vědy"@cs ;
    skos:prefLabel "Sciences humaines"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:L
    a skos:Concept ;
    skos:notation "L" ;
    skos:prefLabel "Information Science"@en ;
    skos:prefLabel "Informační vědy"@cs ;
    skos:prefLabel "Sciences de l'information"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:M
    a skos:Concept ;
    skos:notation "M" ;
    skos:prefLabel "Named Groups"@en ;
    skos:prefLabel "Lidé"@cs ;
    skos:prefLabel "Individus"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:N
    a skos:Concept ;
    skos:notation "N" ;
    skos:prefLabel "Health Care"@en ;
    skos:prefLabel "Zdravotní péče"@cs ;
    skos:prefLabel "Catégorie soins de santé"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:V
    a skos:Concept ;
    skos:notation "V" ;
    skos:prefLabel "Publication Characteristics"@en ;
    skos:prefLabel "Publikační charakteristiky"@cs ;
    skos:prefLabel "Caractéristiques d'une publication"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:Y
    a skos:Concept ;
    skos:notation "Y" ;
    skos:prefLabel "Qualifiers (Subheadings)"@en ;
    skos:prefLabel "Kvalifikátory (podhesla)"@cs ;
    skos:prefLabel "Qualifiers (Subheadings)"@fr ;
    skos:topConceptOf meshx:scheme .

meshx:Z
    a skos:Concept ;
    skos:notation "Z" ;
    skos:prefLabel "Geographicals"@en ;
    skos:prefLabel "Geografická místa"@cs ;
    skos:prefLabel "Lieux géographiques"@fr ;
    skos:topConceptOf meshx:scheme .

meshv:TopicalDescriptor
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Topical Descriptor"@en ;
  rdfs:label "Tematické heslo"@cs ;
  rdfs:label "Descripteur thématique"@fr .

meshv:PublicationType
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Publication Type"@en ;
  rdfs:label "Publikační typ"@cs ;
  rdfs:label "Type de publication"@fr .

meshv:CheckTag
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Check Tag"@en ;
  rdfs:label "Check tag"@cs ;
  rdfs:label "Check tag"@fr .

meshv:GeographicalDescriptor
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Geographical Descriptor"@en ;
  rdfs:label "Geografické heslo"@cs ;
  rdfs:label "Descripteur géographique"@fr .

meshv:Qualifier
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Subheading"@en ;
  rdfs:label "Podheslo"@cs ;
  rdfs:label "Qualifier"@fr .

skosext:DeprecatedConcept
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Deprecated Concept"@en ;
  rdfs:label "Zastaralý koncept"@cs ;
  rdfs:label "Concept obsolète"@fr .
